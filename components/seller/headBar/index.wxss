.head-box {
  width: 100%;
  height: 80rpx;
  padding-top: 44rpx;
  padding-bottom: 1.5vh;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
}
.head-back {
  position: absolute;
  left: 33rpx;
  bottom: 2.8vh;
  z-index: 10;
  width: 20rpx;
  height: 36rpx;
}
.head-back image {
  width: 20rpx;
  height: 36rpx;
}
.head-btn {
  width: 320rpx;
  height: 60rpx;
  display: flex;
  border-radius: 10rpx;
  border: 1rpx solid #ffd339;
  overflow: hidden;
}
.head-btn navigator  {
  flex: 1;
}
.head-btn view {
  width: 100%;
  text-align: center;
  line-height: 60rpx;
  font-size: 30rpx;
  color: #333;
  box-sizing: border-box;
}
.head-btn .checked {
  background: #FFD339;
}

.nav-content{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  min-height: 128rpx;
  background: #fff;
  z-index: 10;
}
.nav-warp{
  width: 100%;
  height: 88rpx;
  background: #fff;
  position: relative
}
.nav-left{
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  width: 60rpx;
  height: 58rpx;
  padding-top: 30rpx;
  padding-left: 30rpx;
}
.nav-title{
  width: 100%;
  height: 88rpx;
  text-align: center;
  line-height: 88rpx;
  color: #333333;
}
.nav-sticky{
  width: 100%;
}