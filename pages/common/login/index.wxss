.login {
  width: 100%;
  height: 100vh;
  background: #fff;
  position: relative;
}
.login .logo {
  width: 100%;
  background: #FFF2C3;
  padding-top: 140rpx;
  padding-bottom: 73rpx;
  text-align: center;
}
.logo image {
  width: 435rpx;
  height: 127rpx;
}
.login-line {
  width: 100%;
  height: 87rpx;
  margin-bottom: 63rpx;
}
.login button::after {
  border: 0!important;
}
.wx-login {
  width: 500rpx;
  height: 88rpx;
  background: #ffd339;
  border-radius: 10rpx;
  font-size: 32rpx;
  color: #333;
  font-weight: 400;
  line-height: 88rpx;
}
.protocol {
  width: 100%;
  text-align: center;
  color: #999;
  font-size: 28rpx;
  position: absolute;
  bottom: 53rpx;
}
