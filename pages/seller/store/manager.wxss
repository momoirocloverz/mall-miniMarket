.container {
  width: 100%;
  min-height: 100vh;
  background: #F5F7FA;
  padding-top: 20rpx;
  box-sizing: border-box;
  position: relative;
}
.content {
  width: 100%;
  background: #fff;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.cont-item {
  border-bottom: 1rpx solid #F5F7FA;
  padding: 34rpx 0;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.cont-item text:first-child {
  font-size: 30rpx;
  font-weight: 400;
  color: #333333;
}
.cont-item .value {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.cont-item .value text:first-child {
  width: 420rpx;
  text-align: right;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-right: 10rpx;
}
.cont-item .value image {
  width: 90rpx;
  height: 90rpx;
  border-radius: 10rpx;
  margin-right: 10rpx;
}
.contact {
  width: 100%;
  position: absolute;
  bottom: 50rpx;
  left: 0;
}
.contact view {
  text-align: center;
  line-height: 60rpx;
  font-size: 28rpx;
  color: #999;
  font-weight: 400;
}
.contact text{
  color: 	#00BFFF;
}